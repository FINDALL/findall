 			<script type="text/javascript">
 			  jQuery( function ( $ ) {
 			    $( ".slide_banner_new" ).each( function () {
 			      var maskWd = $( ".mask", this ).width();
 			      var $slide = $( ".mask ul", this );
 			      var $page = $( ".page", this );
 			      var listLength = $( "li", $slide ).length;
 			      var ulMoving = false;
 			      var objRun;
 			      $slide.width( maskWd * listLength );
 			      $( ".total", $page ).html( listLength );
 			      function next() {
 			        if ( !ulMoving && listLength > 1 ) {
 			          ulMoving = true;
 			          var now = $( ".now", $page ).html() * 1;
 			          if ( now < listLength ) {
 			            $( ".now", $page ).html( now + 1 );
 			          } else {
 			            $( ".now", $page ).html( "1" );
 			          }
 			          $slide.animate( {
 			            left: "-100%"
 			          }, 500, function () {
 			            $( "li:first", this ).appendTo( $slide )
 			            $( this ).css( { "left": "0" } )
 			            ulMoving = false;
 			          } );
 			        }
 			      }
 			      function prev() {
 			        if ( !ulMoving && listLength > 1 ) {
 			          ulMoving = true;
 			          var now = $( ".now", $page ).html() * 1;
 			          if ( now > 1 ) {
 			            $( ".now", $page ).html( now - 1 );
 			          } else {
 			            $( ".now", $page ).html( listLength );
 			          }
 			          $( "li:last", $slide ).prependTo( $slide )
 			          $slide.css( { "left": "-100%" } )
 			          $slide.animate( {
 			            left: "0"
 			          }, 500, function () {
 			            ulMoving = false;
 			            $( "li:first", $slide ).prependTo( $slide )
 			          } );
 			        };
 			      };
 			      function autoRun() {
 			        objRun = setInterval( function () {
 			          next();
 			        }, 4000 );
 			      };
 			      function stop() {
 			        clearInterval( objRun );
 			      };

 			      $slide.find( "li:nth-child(2)" ).focus( function () {
 			        $( "li:first", this ).appendTo( $slide );
 			        //alert()
 			      } );

 			      $( '.next', this ).click( function () {
 			        next(), stop();
 			      } );
 			      $( '.prev', this ).click( function () {
 			        prev(), stop();
 			      } );
 			      autoRun();
 			      $( this ).mouseenter( stop ).mouseleave( autoRun )
 			      $( this ).focusin( stop )
 			      $( this ).focusout( autoRun )

 			    } );
 			  } )
			</script>
